function nvr foo = (byte iterations)
{
	byte n0 = 0;
	byte n1 = 1;

	byte iteration = 0;
	while (iteration < iterations)
	{
		print(n0);
		byte n2 = n0 + n1;
		n0 = n1;
		n1 = n2;

		iteration = iteration + 1;
	}
}

function byte bar = (byte powers)
{
	byte n = 1;

	byte power = 0;
	while (power < powers)
	{
		print(n);
		n = n + n;
		power = power + 1;
	}

	byte toSubtract = 1;

	power = power + 1;

	while (power > 0)
	{
		print(n);
		toSubtract = toSubtract + toSubtract;
		n = n - toSubtract;
		power = power - 1;
	}
}

function byte main = ()
{
	bar(20);
	return 0;
}
